{
	"description": "BuddyBoss adds community features to WordPress. Member Profiles, Activity Streams, Direct Messaging, Notifications, and more!",
	"devDependencies": {
		"@wordpress/api-fetch": "^5.2.6",
		"@wordpress/babel-preset-default": "^7.32.0",
		"@wordpress/browserslist-config": "^5.31.0",
		"@wordpress/components": "^19.17.0",
		"@wordpress/element": "^4.20.0",
		"@wordpress/env": "^10.8.0",
		"@wordpress/i18n": "^4.0.0",
		"@wordpress/icons": "^9.0.0",
		"@wordpress/media-utils": "^4.19.0",
		"@wordpress/scripts": "^26.19.0",
		"@wordpress/stylelint-config": "^19.1.0",
		"apidoc": "^0.22.0",
		"concurrently": "^8.2.2",
		"css-loader": "^6.11.0",
		"grunt": "^1.4.1",
		"grunt-apidoc": "^0.11.0",
		"grunt-check-dependencies": "~1.0.0",
		"grunt-checktextdomain": "~1.0.1",
		"grunt-contrib-clean": "~2.0.0",
		"grunt-contrib-compress": "^2.0.0",
		"grunt-contrib-copy": "~1.0.0",
		"grunt-contrib-cssmin": "^4.0.0",
		"grunt-contrib-imagemin": "^4.0.0",
		"grunt-contrib-jshint": "~3.1.1",
		"grunt-contrib-uglify": "~5.0.1",
		"grunt-contrib-watch": "^1.1.0",
		"grunt-exec": "~3.0.0",
		"grunt-json2php": "^0.1.4",
		"grunt-jsvalidate": "~0.2.2",
		"grunt-legacy-util": "^2.0.1",
		"grunt-patch-wordpress": "^3.0.1",
		"grunt-rtlcss": "^2.0.2",
		"grunt-sass": "~3.1.0",
		"grunt-string-replace": "^1.3.1",
		"grunt-stylelint": "^0.17.0",
		"grunt-wp-i18n": "^1.0.3",
		"husky": "^7.0.4",
		"jira-prepare-commit-msg": "^1.7.2",
		"matchdep": "^2.0.0",
		"node-sass": "^7.0.0",
		"postcss": "^8.4.4",
		"postcss-scss": "^4.0.2",
		"react-beautiful-dnd": "^13.1.1",
		"sass": "^1.71.0",
		"sass-loader": "^13.3.3",
		"style-loader": "^3.3.4",
		"stylelint": "^13.13.1"
	},
	"scripts": {
		"prepare": "npm install husky jira-prepare-commit-msg --save-dev && npx husky install",
		"build:blocks": "npm run build:block:core",
		"build:block:core": "wp-scripts build --webpack-src-dir=src/js/blocks/bp-core --config src/js/blocks/bp-core/webpack.config.js",
		"watch:readylaunch-header": "wp-scripts start --webpack-src-dir=src/js/blocks/bp-core/readylaunch-header --config src/js/blocks/bp-core/webpack.config.js",
		"build:admin": "npm run build:admin:readylaunch && npm run build:admin:rl-onboarding",
		"build:admin:readylaunch": "BUILD_TARGET=readylaunch wp-scripts build --webpack-src-dir=src/js/admin/readylaunch --config src/js/admin/webpack.config.js && npm run build:admin:readylaunch:scss",
		"build:admin:readylaunch:scss": "sass --style=compressed --no-source-map src/js/admin/readylaunch/styles/scss/settings.scss:src/bp-core/admin/bb-settings/readylaunch/build/styles/settings.css",
		"build:admin:rl-onboarding": "BUILD_TARGET=rl-onboarding wp-scripts build --webpack-src-dir=src/js/admin/rl-onboarding --config src/js/admin/webpack.config.js && npm run build:admin:rl-onboarding:scss",
		"build:admin:rl-onboarding:scss": "sass --style=compressed --no-source-map src/js/admin/rl-onboarding/styles/onboarding.scss:src/bp-core/admin/bb-settings/rl-onboarding/build/onboarding.css",
		"watch:admin:readylaunch": "concurrently \"npm run watch:admin:readylaunch:scss\" \"BUILD_TARGET=readylaunch wp-scripts start --webpack-src-dir=src/js/admin/readylaunch --config src/js/admin/webpack.config.js\"",
		"watch:admin:readylaunch:scss": "sass --watch src/js/admin/readylaunch/styles/scss/settings.scss:src/bp-core/admin/bb-settings/readylaunch/build/styles/settings.css",
		"watch:admin:rl-onboarding": "concurrently \"npm run watch:admin:rl-onboarding:scss\" \"BUILD_TARGET=rl-onboarding wp-scripts start --webpack-src-dir=src/js/admin/rl-onboarding --config src/js/admin/webpack.config.js\"",
		"watch:admin:rl-onboarding:scss": "sass --watch src/js/admin/rl-onboarding/styles/onboarding.scss:src/bp-core/admin/bb-settings/rl-onboarding/build/onboarding.css"
	},
	"engines": {
		"node": ">=14.15.0"
	},
	"license": "GPL-2.0-or-later",
	"version": "3.1.0",
	"BBVersion": "2.14.1",
	"dependencies": {
		"embla-carousel-auto-height": "^8.6.0",
		"embla-carousel-react": "^8.6.0"
	}
}
